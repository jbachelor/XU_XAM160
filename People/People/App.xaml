    x:Class="People.App">
    <Application.Resources>
        <ResourceDictionary>
            <Style
                TargetType="Label">
                <Setter
                    Property="FontAttributes"
                    Value="Bold" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
